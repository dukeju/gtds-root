<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping package="com.brother.gtds.model">
	<class name="Schedule" table="schedule">
		<id name="id" type="integer" column="id">
			<generator class="identity"></generator>
		</id>

		<many-to-one name="department" class="Department" column="dept_id"></many-to-one>		
		
		<property name="stuProposeBegin" column="stu_propose_begin" type="date"></property>
		<property name="proposeBegin" column="propose_begin" type="date"></property>
		<property name="proposeExpiry" column="propose_expiry" type="date"></property>
		<property name="selectBegin" column="select_begin" type="date"></property>
		<property name="selectEnd" column="select_end" type="date"></property>
		
	</class>
</hibernate-mapping>